{
  "_version": "1.73.1",
  "sap.app": {
    "id": "timesheetodm",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.19.6",
      "toolsId": "a7ca8898-d909-40f7-b2a0-09a48d5f7e2e"
    },
    "dataSources": {
      "annotation": {
        "type": "ODataAnnotation",
        "uri": "annotations/annotation.xml",
        "settings": {
          "localUri": "annotations/annotation.xml"
        }
      },
      "mainService": {
        "uri": "/sap/opu/odata4/sap/zui_tts_odm_o4/srvd/sap/zui_tts_odm_o4/0001/",
        "type": "OData",
        "settings": {
          "annotations": [
            "annotation"
          ],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "saisieodm-manage": {
          "semanticObject": "saisieodm",
          "action": "manage",
          "title": "{{saisieodm-manage.flpTitle}}",
          "subTitle": "{{saisieodm-manage.flpSubtitle}}",
          "icon": "sap-icon://timesheet",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "extends": {
      "extensions": {
        "sap.ui.controllerExtensions": {
          "sap.fe.templates.ListReport.ListReportController": {
            "controllerName": "timesheetodm.ext.controller.SetDelete"
          }
        }
      }
    },
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.136.11",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "timesheetodm.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "ZC_TTS_ODMList",
          "target": "ZC_TTS_ODMList"
        },
        {
          "pattern": "ZC_TTS_ODM({key}):?query:",
          "name": "ZC_TTS_ODMObjectPage",
          "target": "ZC_TTS_ODMObjectPage"
        }
      ],
      "targets": {
        "ZC_TTS_ODMList": {
          "type": "Component",
          "id": "ZC_TTS_ODMList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/ZC_TTS_ODM",
              "variantManagement": "Page",
              "navigation": {
                "ZC_TTS_ODM": {
                  "detail": {
                    "route": "ZC_TTS_ODMObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable"
                  },
                  "columns": {
                    "CustomColumnRating": {
                      "header": "Supprimer Inline",
                      "width": "10em",
                      "importance": "High",
                      "horizontalAlign": "Center",
                      "position": {
                        "placement": "Before"
                      },
                      "template": "timesheetodm.ext.fragment.customdeletebutton",
                      "availability": "Default"
                    }
                  }
                }
              }
            }
          }
        },
        "ZC_TTS_ODMObjectPage": {
          "type": "Component",
          "id": "ZC_TTS_ODMObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/ZC_TTS_ODM",
              "content": {
                "footer": {
                  "actions": {
                    "Confirmation": {
                      "press": "timesheetodm.ext.controller.Confirmation.postConfirmation",
                      "visible": true,
                      "enabled": "{= ${ui>/editMode} !== 'Editable'}",
                      "requiresSelection": false,
                      "text": "Post"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
